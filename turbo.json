{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "globalEnv": [
    "NEXT_PUBLIC_SUPABASE_URL",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY",
    "SUPABASE_SERVICE_ROLE_KEY",
    "NEXT_PUBLIC_CRYPTOMUS_API_BASE",
    "CRYPTOMUS_MERCHANT_UUID",
    "CRYPTOMUS_API_KEY",
    "RESEND_API_KEY",
    "TELEGRAM_BOT_TOKEN",
    "NEXT_PUBLIC_APP_BASE_URL",
    "NEXT_PUBLIC_RENT_BASE_URL"
   
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "SUPABASE_SERVICE_ROLE_KEY",
        "NEXT_PUBLIC_CRYPTOMUS_API_BASE",
        "NEXT_PUBLIC_APP_BASE_URL",
        "NEXT_PUBLIC_RENT_BASE_URL"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "SUPABASE_SERVICE_ROLE_KEY",
        "NEXT_PUBLIC_CRYPTOMUS_API_BASE",
        "CRYPTOMUS_MERCHANT_UUID",
        "CRYPTOMUS_API_KEY",
        "RESEND_API_KEY",
        "TELEGRAM_BOT_TOKEN",
        "NEXT_PUBLIC_APP_BASE_URL",
        "NEXT_PUBLIC_RENT_BASE_URL"
     
      ]
    }
  }
}
